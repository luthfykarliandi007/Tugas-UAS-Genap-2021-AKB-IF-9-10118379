<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/splash"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key"/>

        <activity android:name="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379.View.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379.View.OnBoardSliderActivity"></activity>
        <activity android:name="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379.MainActivity"></activity>
        <activity android:name="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379.View.DetailActivity"/>
        <activity android:name="com.luthfy.tugas_uas_genap_2021_akb_if_9_10118379.View.MapActivity"/>
    </application>

</manifest>